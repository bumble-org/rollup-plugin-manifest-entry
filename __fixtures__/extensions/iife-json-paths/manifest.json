{
  "name": "iife-json-paths-1",
  "description": "iife json paths example 1",
  "manifest_version": 2,
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.google.com/*"]
    },
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": ["https://www.yahoo.com/*"]
    }
  ]
}
